<div class="panel panel-midnightblue">
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-6 col-sm-offset-3">
                <div class="btn-toolbar">
                    <input type="button" class="btn btn-primary" value="Save">                                                                     
                </div>
            </div>
        </div>
        <div class="row">
            <form>
                <fieldset id="pictures">                    
                    <span>Drag and drop any images files to this form to attach images </span>
                    <span>Or choose a local file to upload:<input class="ignoreField" type="file" name="imageFileInputImg" id="fileChoose"/>
                        <input  type="button" name="imageUploadBtn" value="Upload Image/File(s)" class="imageUpload"/><br/></span>
                    <div id="image_containerImg" class="ImgPlaceHolder ddplaceHolder"></div>
                </fieldset>
            </form>
        </div>
    </div>
</div>
